<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="RedisBenchmark" type="System.Configuration.NameValueSectionHandler" />
  </configSections>

  <RedisBenchmark>
    <add key="distributorHost" value="localhost" />
    <add key="distributorPort" value="20000" />
    <add key="countTreads" value="1" />
  </RedisBenchmark>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>